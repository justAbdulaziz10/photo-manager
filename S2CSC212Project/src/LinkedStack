public class LinkedStack<T> implements Stack<T> {
	private Node<T> top;

	public LinkedStack() {
		top = null;
	}

	@Override
	public T pop() {
		T e = top.data;
		top = top.next;
		return e;
	}

	@Override
	public void push(T e) {
		Node<T> tmp = new Node<T>(e);
		tmp.next = top;
		top = tmp;
	}

	@Override
	public boolean empty() {
		return top==null;
	}

	@Override
	public boolean full() {
		return false;
	}
	
	
		
}
